{
    "version": 2,
    "public": true,
    "builds": [
        {"src": "index.js", "use": "@now/node"},
        {"src": "routes/**/*.js", "use":"@now/node"}
    ],
    "routes": [
        {"src": "/auth/google", "dest": "/routes/auth/index.js"},
        {"src": "/auth/google/redirect", "dest": "/routes/callback/index.js"},
        {"src": "/logout", "dest": "/routes/logout/index.js"}
    ]
}
	