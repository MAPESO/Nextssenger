
{
    "alias": "auth-chat.now.sh",
    "version": 2,
    "name": "auth-chat",
    "public": true,
    "builds": [
        {"src": "index.js", "use": "@now/node-server"}
    ],
    "routes": [
        {"src": "/.*", "dest": "index.js"}
    ],
    "env": {
        "GOOGLE_ID": "@google_id",
        "GOOGLE_SECRET": "@google_secret",
        "SESSION_SECRET": "@session_secret"
    }
}