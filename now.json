{
    "version": 2,
    "public": true,
    "alias": "auth-chat",
    "builds": [
        {"src": "index.js", "use": "@now/node"},
        {"src": "auth.js", "use": "@now/node"},
        {"src": "cb.js", "use": "@now/node"}
    ],
    "routes": [
        {"src": "/auth/google", "dest": "auth.js"},
        {"src": "/auth/google/callback", "dest": "cb.js"}
    ],
    "env": {
        "GOOGLE_ID": "@google_id",
        "GOOGLE_SECRET": "@google_secret",
        "KEY_1": "@key_1",
        "KEY_2": "@key_2",
        "URL_CALLBACK": "@url_callback"
    }
}
