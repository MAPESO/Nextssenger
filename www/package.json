{
  "name": "www",
  "version": "1.0.0",
  "description": "frontend from nextssenger",
  "main": "index.js",
  "author": {
    "name": "Markoz Pe√±a Mendez",
    "email": "markdrew53@gmail.com",
    "twitter": "@markdrew53"
  },
  "license": "MIT",
  "private": false,
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 10,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "es6": true,
      "node": true
    },
    "extends": [
      "prettier",
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "plugins": [
      "prettier",
      "react"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true
        }
      ]
    }
  },
  "dependencies": {
    "@pusher/chatkit-client": "^1.7.1",
    "cookies": "^0.7.3",
    "jsonwebtoken": "^8.5.1",
    "next": "^8.1.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@zeit/next-css": "^1.0.1",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.2",
    "husky": "^2.7.0",
    "lint-staged": "^8.2.1",
    "prettier": "^1.18.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "scripts": {
    "test": "eslint .",
    "now-dev": "now dev -p 8080",
    "dev": "next dev -p $PORT",
    "build": "next build",
    "now-build": "npm run build"
  }
}
